<li id="<%= dom_id(room) %>" class="col-span-1 flex shadow-sm rounded-md border border-gray-200">
  <%= link_to room_path(room), class: 'bg-blue-200 flex-1 flex items-center justify-between bg-white rounded-md truncate' do %>
    <div class="flex-1 px-4 py-2 text-sm truncate">
      <span class="text-gray-900 font-medium hover:text-gray-600"><%= room.name %></span>
      <p class="text-gray-500">16 Members</p>
    </div>
  <% end %>
</li>
